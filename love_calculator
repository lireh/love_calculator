def love_calculator():
    print("Привет! Давай узнаем насколько вы совместимы с другим человеком!")
    name1 = input("Введите ваше имя: ")
    name2 = input("Введите имя другого человека: ")

    # Некоторые алгоритмы расчета совместимости могут использовать хэши или другие методы
    # Я использую простую формулу для демонстрационных целей

    compatibility = (len(name1) + len(name2)) % 101
    print(f"Ваша совместимость с {name2} составляет {compatibility}%.")

def main():
    love_calculator()

if __name__ == '__main__':
    main()
